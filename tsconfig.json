{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@design-system": ["./src/common/components"],
      "@/utils/*": ["./src/utils/*"],
      "@/common/*": ["./src/common/*"],
      "@/modules/*": ["./src/modules/*"],
      "@/style/*": ["./src/style/*"],
      "@/assets/*": ["./assets/*"],
      "@/routes/*": ["./src/routes/*"],
      "@/store/*": ["./src/store/*"]
    }
  }
}
